<!-- Page Content Area
===================================== -->
<section id="contact-page" class="ptb-100">
	<div class="container">
		
		<div class="row pt-70">
			<div class="col-md-6">
				<div class="reply-form">
								
					<!-- Post Form -->
					<form class="form contact-us-form" name="contactform" id='contact_form' method="post" action='/login'>
						<div class="clearfix">
							<div id="contact_message"></div>
							
							<div class="col-xs-6">
								<!-- Name -->
								<div class="form-group inner-addon left-addon">
									<label for="exampleInputEmail1">UserName</label>
									<i class="fa fa-user" aria-hidden="true"></i>
									<input type="text" name="userName" class="form-control gardener-reply-form" id="exampleInputEmail1" pattern=".{2,100}" required>
								</div>
							</div>
							<div class="col-xs-6">
								<!-- Email -->
								<div class="form-group inner-addon left-addon">
									<label for="exampleInputEmail2">RealName</label>
									<i class="fa fa-user" aria-hidden="true"></i>
                                    <input type="text" name="realName" class="form-control gardener-reply-form" id="exampleInputEmail1" pattern=".{2,100}" required>
								</div>
							</div>
							
							<div class="col-xs-12">
								<!-- Send Button -->
								<button class="btn btn-default">Sign In or Up</button>
							</div>
						</div>
						
						
						
						
					</form>
				</div>
			</div>
		</div>
	</div>
</section>
